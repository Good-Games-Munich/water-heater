{
  "name": "water-heater",
  "version": "0.0.9",
  "description": "A api and discord bot for Good Games Munich",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepare": "husky install"
  },
  "author": "Luis Beu <me@luisbeu.de> (https://luisbeu.de/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Good-Games-Munich/water-heater.git"
  },
  "keywords": [
    "nestjs",
    "discord",
    "bot"
  ],
  "bugs": {
    "url": "https://github.com/Good-Games-Munich/water-heater/issues"
  },
  "homepage": "https://github.com/Good-Games-Munich/water-heater#readme",
  "lint-staged": {
    "*.{js,ts,json,yml,yaml}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@nanogiants/nestjs-swagger-api-exception-decorator": "1.6.11",
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.7",
    "@nestjs/platform-fastify": "10.2.7",
    "@nestjs/typeorm": "10.0.0",
    "discord.js": "14.13.0",
    "i18next": "23.5.1",
    "necord": "6.1.8",
    "pg": "8.11.3",
    "ts-error": "1.0.6",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@beuluis/eslint-config": "2.0.1",
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "better-sqlite3": "8.7.0",
    "eslint": "8.51.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "14.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
